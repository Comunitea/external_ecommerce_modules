<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <!-- Add module styles & scripts to website head -->
    <template id="ccb_website_assets_frontend" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/less" href="/codecoupon_base/static/css/codecoupon.less"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
			<script type="text/javascript" src="/codecoupon_base/static/js/codecoupon.js"/>
		</xpath>
    </template>

</odoo>