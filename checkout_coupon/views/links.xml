<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="styles_adds" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link href="/checkout_coupon/static/css/checkout.less" rel="stylesheet" type="text/less"/>
        </xpath>
    </template>

    <template id="script_adds" inherit_id="website.layout">
        <xpath expr="//div[@id='wrapwrap']" position="inside">
            <script src="/checkout_coupon/static/js/checkout.js" type="text/javascript"/>
        </xpath>
    </template>

</odoo>