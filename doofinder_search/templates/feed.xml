<t t-raw="values['meta_title']"/>|
<t t-raw="values['meta_description']"/>|
<t t-raw="values['image_link']"/>|
<t t-raw="values['categories']"/>|
<t t-esc="values['availability']"/>|
<t t-esc="values['brand']"/>|
<t t-esc="values['mpn']"/>|
