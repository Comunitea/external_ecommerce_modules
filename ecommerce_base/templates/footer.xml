<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- Delayed charge of own js at the end of pages -->
    <template id="website_layout" name="Main layout Ecommerce Base" inherit_id="website.layout">
         <!-- Add to bottom of page -->
        <xpath expr="//div[@id='wrapwrap']" position="after">
            <script src="/ecommerce_base/static/js/jquery.validate.min.js" type="text/javascript"/>
            <script src="/ecommerce_base/static/js/main.js" type="text/javascript"/>
        </xpath>
    </template>

</odoo>