<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!-- Custom Assets -->
    <template id="website_assets_frontend" name="Website Assets eCommerce Extend" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link href="/website_ecommerce_extend/static/scss/contactus.scss" rel="stylesheet" type="text/scss"/>
            <link href="/website_ecommerce_extend/static/scss/portal.scss" rel="stylesheet" type="text/scss"/>
        </xpath>
    </template>

    <!-- Variables to access via bootstrap_overriden file -->
    <template id="_assets_frontend_helpers" inherit_id="web._assets_frontend_helpers">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/website_ecommerce_extend/static/scss/bootstrap_overridden.scss"/>
        </xpath>
    </template>

</odoo>
