#. module: payment_paga_mas_tarde
#: model_terms:payment.acquirer,cancel_msg:payment_paga_mas_tarde.payment_acquirer_pmt
msgid "<span><i>Cancel,</i> Your payment has been cancelled.</span>"
msgstr "<span><i>Cancelado,</i> Su pago ha sido cancelado.</span>"

#. module: payment_paga_mas_tarde
#: model_terms:payment.acquirer,done_msg:payment_paga_mas_tarde.payment_acquirer_pmt
msgid "<span><i>Done,</i> Your online payment has been successfully processed. Thank you for your order.</span>"
msgstr "<span><i>Aprobado,</i> su pago en línea ha sido porcesado satisfactoriamente. Gracias por su orden.</span>"

#. module: payment_paga_mas_tarde
#: model_terms:payment.acquirer,error_msg:payment_paga_mas_tarde.payment_acquirer_pmt
msgid "<span><i>Error,</i> Please be aware that an error occurred during the transaction. The order has been confirmed but will not be paid. Do not hesitate to contact us if you have any questions on the status of your order.</span>"
msgstr "<span><i>Error,</i> Tenga en cuenta que se produjo un error durante la transacción. El pedido ha sido confirmado pero no ha sido pagado. No dude en contactarnos si tiene alguna pregunta sobre el estado de su pedido.</span>"

#. module: payment_paga_mas_tarde
#: model_terms:payment.acquirer,pending_msg:payment_paga_mas_tarde.payment_acquirer_pmt
msgid "<span><i>Pending,</i> Your online payment has been successfully processed. But your order is not validated yet.</span>"
msgstr "<span><i>Pendiente,</i> Su pago en línea ha sido procesado con éxito. Pero tu pedido aún no está validado.</span>"

#. module: payment_paga_mas_tarde
#: model_terms:ir.ui.view,arch_db:payment_paga_mas_tarde.pmt_tx_ko
msgid "<span>Please, contact us if you wish more info about payment conditions</span>"
msgstr "<span>Por favor, contáctenos si necesita más información sobre las condiciones de pago</span>"

